{
    "setupFilesAfterEnv": [
      "<rootDir>/__test__/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "verbose": true
}